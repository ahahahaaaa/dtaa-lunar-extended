{
  "name":        "ahahahaaaa/dtaa-lunar-extended",
  "description": "A library for extended managing lunar ecomerce from dtaa-tech.com",
  "homepage":    "https://dtaa-tech.com/dtaa-lunar-extended",
  "keywords":    ["dtaa", "lunar", "ecomerce", "extended", "storefront"],
  "license":     "MIT",
  "authors": [
    {"name": "DTAA-TECH"}
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    }
  },
  "require": {
    "php": ">=8.1",
    "lunarphp/core": "^0.8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.27"
  },
  "suggest": {
    "ext-intl": "PHP Internationalization Libraries are required to use the SpoofChecking validation"
  },
  "autoload": {
    "psr-0": {
      "Ahahahaaaa\\DtaaLunarExtended\\": "src"
    },
      "extra": {
          "laravel": {
              "providers": [
                  "DtaaLunarExtended\\DtaaLunarServiceProvider"
              ]
          }
      }
  },
  "autoload-dev": {
    "psr-0": {
      "Ahahahaaaa\\DtaaLunarExtended\\Tests\\": "tests"
    }
  }
}
